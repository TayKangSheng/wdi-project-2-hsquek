<h1>Family Account Management</h1>



<% if (message) { %>
  <h2>
    <%= message %>
  </h2>

<% } %>

<% if (family) { %>
<a class="btn btn-primary" href="/family/<%= family.id %>/pushuser">Update users</a>
<a class="btn btn-primary" href="/family/<%= family.id %>/deleteuser">Delete user</a>

<% } else { %>

<h4><a href="/family/create">Create family account</a></h4>

<% } %>
